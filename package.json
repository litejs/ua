{
  "name": "@litejs/ua",
  "version": "22.3.0",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "Simple User-Agent parser",
  "keywords": [
    "user-agent",
    "litejs"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": "github:litejs/ua",
  "scripts": {
    "test": "c8 -r lcov -r text-summary --check-coverage --100 lj test --brief test/index.js && jshint *.js"
  },
  "devDependencies": {
    "@litejs/cli": "23.2.0",
    "c8": "7.12.0",
    "jshint": "2.13.6"
  },
  "litejs": {
    "build": false,
    "install": false
  },
  "jshintConfig": {
    "esversion": 5,
    "asi": true,
    "evil": true,
    "laxcomma": true,
    "maxdepth": 6,
    "nonbsp": true,
    "undef": true,
    "unused": true,
    "shadow": "outer",
    "quotmark": "double"
  }
}
